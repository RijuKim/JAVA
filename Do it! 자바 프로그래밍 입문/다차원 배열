package array;

public class TowDimension {

	public static void main(String[] args) {
		
		int [][] arr= {{1, 2, 3}, {4, 5, 6}}; //2행 3열 1차원 배열이 여러 개 있다고 생각
		
		System.out.println(arr.length); //2(행)
		System.out.println(arr[0].length); //3(열)
		
		for(int i=0; i<arr.length; i++) {
			for(int j=0; j<arr[i].length; j++) {
				System.out.println(arr[i][j]);
			}
			System.out.println();
		}
	}

}
