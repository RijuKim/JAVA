package assignment;

public class OperationEx {
	
	public static void main(String[] args) {
		int age =24;
		System.out.println(age);
		
		int num=10;
		int num2= -num; // 부호연산자 사용 시 num의 값 자체가 음수로 바뀌지는 않는다.
		System.out.println(num); //10 출력
		System.out.println(num2); //-10 출력
		
		num=-num; //변수의 값을 바꾸려면 대입연산자를 써야함.
		System.out.println(num); //-10 출력
				
	}

}
