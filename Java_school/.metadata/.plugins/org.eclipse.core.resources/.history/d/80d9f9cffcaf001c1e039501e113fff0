package xyz;

public class Z extends Y {
	int i = 30;
	String msg = "I am a Z.";

	void print() {
		System.out.println(msg);
	}

	void play() {
		System.out.println("Play.." + msg);
	}

	void doZ() {
		System.out.println("do somethind in Z.");
	}

	void test(int i) {
		Z z = new Z();
		Y y = z;
		X x = z;
		z.print();
		y.print();
		super.print();
		play();
		super.play();

		System.out.println("\ni = " + i);
		System.out.println("this.i = " + this.i);
		System.out.println("super.i = " + super.i);

	}
}
