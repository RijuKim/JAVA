package week5;

public class Teacher extends Members {
	String dept;
	Members students[];

	public Teacher(String name, int idnum, String dept) {
		super(name, idnum, dept);
	}

	public void setStudents(Members sub[]) {
		students = sub;
	}
	
	public void work() {
		System.out.println("Teacher: \""+name+"(ID:"+id+")"+
		"\" teachers with his students in "+dept+" dept.");
	}
}
